<%= form_with url: "/gyms", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :location %>
  <%= form.text_field :location %>
  <%= form.label :franchise_owner %>
  <%= form.text_field :franchise_owner %>

  <%= form.submit "Create Gym" %>
<% end %>