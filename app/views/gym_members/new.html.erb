<%= form_with url: "/gyms/#{@gym.id}/members", method: :post, local: true do |form| %>

  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :age %>
  <%= form.text_field :age %>
  <%= form.label :monthly_dues, "Monthly Dues" %>
  <%= form.text_field :monthly_dues %>
  <%= form.label :program_type, "Program Type" %>
  <%= form.text_field :program_type %>
  <%= form.label :active_member, "Active Membership" %>
  <%= form.text_field :active_member %>
  <%= form.hidden_field :gym_id, value: @gym.id %>
  <%= form.submit "Create Member" %>

<% end %>